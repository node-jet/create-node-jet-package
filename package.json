{
  "name": "create-jetngin-extension",
  "version": "0.0.0",
  "description": "starter extension for jetngin",
  "bin": "dist/generate.js",
  "types": "dist/generate.d.ts",
  "prepublish": "tsc",
  "scripts": {
    "uat": "npm run build && node dist/generate myapp",
    "pr": "npm run build && npm run lint",
    "clean": "tsc --build --clean",
    "build": "tsc --build",
    "watch": "tsc --watch",
    "lint": "eslint . --ext .ts",
    "fix": "eslint --fix --ext .ts .",
    "eslint": "eslint",
    "audit": "npx audit-ci --moderate",
    "test": "npm run pr"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jetngin/create-jetngin-extension.git"
  },
  "keywords": [
    "starter",
    "node",
    "jet",
    "package"
  ],
  "author": "Daniel Samson <contact@nodejet.org>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jetngin/create-jetngin-extension/issues"
  },
  "homepage": "https://nodejet.org",
  "dependencies": {
    "extract-zip": "2.0.1",
    "request": "2.88.2"
  },
  "devDependencies": {
    "@types/chai": "5.0.1",
    "@types/mocha": "10.0.10",
    "@types/mock-fs": "4.13.4",
    "@types/node": "22.13.9",
    "@types/sinon": "17.0.4",
    "@typescript-eslint/eslint-plugin": "8.26.0",
    "@typescript-eslint/parser": "8.26.0",
    "audit-ci": "7.1.0",
    "chai": "5.2.0",
    "eslint": "9.21.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-import-resolver-typescript": "3.8.3",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jsdoc": "50.6.3",
    "eslint-plugin-mocha": "10.5.0",
    "eslint-plugin-sonarjs": "3.0.2",
    "install": "0.13.0",
    "nyc": "17.1.0",
    "sinon": "19.0.2",
    "source-map-support": "0.5.21",
    "ts-node": "10.9.2",
    "typescript": "5.8.2"
  }
}